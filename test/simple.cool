class A inherits B {
  a : Int <- 10;
  f() : Int {{
    while a < 10 loop
      let c : Int <- a * 2 + 1, d : Int <- c + 3 in
        2 + c / 3 * d - 1
    pool;
    { a <- 3 * 5 + ~a / 2; ~a;};
  }};
};

class Main {
  a_obj: A <- new A;
  main() : Object {
    if (3 < 4) then
      a_obj@A.f()
    else
      a_obj.f() + 1
    fi
  };
};
