class A {
  a : Int <- not true;
  b : Bool <- a;
  c : String <- "oh my god";

  f() : Int {
    glorious_method(true, "second argument")
  };

 glorious_method(a:Bool, b:String) : Bool {
    a + b - ~c / not 15 + 3
  };
};

class B inherits A {
  a_more_complicated_object_name : SomeComplexType <- 1;
  b : Bool <- true;
  c : String <- "oh my god";

 maybeEvenMoreGloriousMethod(some_param : Int, more_params:String) : String {
    "this one is glorious"
  };
};
